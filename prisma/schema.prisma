datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Example {
    id          String   @id @default(cuid())
    title       String
    description String
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
}

